using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace Converter
{
    public partial class No_Problem : Form
    {
        public No_Problem()
        {
            InitializeComponent();
        }

        private void label1_Click(object sender, EventArgs e)
        {
            
        }

        private void but_yes_Click(object sender, EventArgs e)
        {
            DialogResult result = MessageBox.Show("ТЫ МОЖЕШЬ ЕЕ РЕШИТЬ?", "ПРОБЛЕМА ЕСТЬ", MessageBoxButtons.YesNo);

            if(result == DialogResult.Yes)
                MessageBox.Show("ТОГДА И НЕ ПАРЬСЯ!!!)))", "ПРОБЛЕМЫ НЕТ");
            else
                MessageBox.Show("ТОГДА И НЕ ПАРЬСЯ!!!)))", "ПРОБЛЕМЫ НЕТ");
        }

        private void but_no_Click(object sender, EventArgs e)
        {
            MessageBox.Show("ТОГДА И НЕ ПАРЬСЯ!!!)))","ПРОБЛЕМЫ НЕТ");
        }
    }
}
